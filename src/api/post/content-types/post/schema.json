{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "content": {
      "type": "richtext"
    },
    "date": {
      "type": "datetime"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "uid": {
      "type": "uid",
      "required": true
    },
    "types": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::type.type",
      "inversedBy": "posts"
    }
  }
}
